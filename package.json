{
  "name": "unit-tests-practice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "mocha && c8 report --reporter=html",
    "coverage": "c8 mocha",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "build": "babel src --out-dir dist --copy-files",
    "precommit-check": "npm run lint && npm run test",
    "quick-check": "npm run lint && npm test",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm test"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@eslint/js": "^9.38.0",
    "c8": "^10.1.3",
    "chai": "^6.2.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "mocha": "^11.7.4"
  }
}